{
	"apiVersion": "v1",
	"kind": "Pod",
	"metadata": {
		"name": "{{ name }}",
		"labels": {
			"cube-harvest.io/unit-type": "{{ unit_type }}"
		}
	},
	"spec": {
		"containers": [{
			"name": "astro-unit",
			"image": "registry.cube-harvest.invalid/miner:42",
			"env": [{
				"name": "TARGET",
				"value": "{{ target_ip }}"
			}]
		}]
	}
}
